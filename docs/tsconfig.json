{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "types": [
      "node",
      "react",
      "react-dom"
    ],
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.mdx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
