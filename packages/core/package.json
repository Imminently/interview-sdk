{
  "name": "@imminently/interview-sdk",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/imminently/interview-sdk.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "bump:patch": "npm run build &&npm version patch --no-git-tag-version",
    "publish:patch": "npm run bump:patch && npm publish"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pako": "^2.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.3",
    "@types/pako": "^2.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
