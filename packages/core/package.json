{
  "name": "@imminently/interview-sdk",
  "version": "0.5.7",
  "repository": {
    "type": "git",
    "url": "git://github.com/imminently/interview-sdk.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "check:fix": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check --write .",
    "check": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check .",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "bump:patch": "npm run build && npm version patch --no-git-tag-version",
    "deploy": "bun run bump:patch && npm publish"
  },
  "dependencies": {
    "@dagrejs/graphlib": "^2.2.4",
    "@date-fns/utc": "^2.1.1",
    "axios": "^1.13.1",
    "date-fns": "^4.1.0",
    "immer": "^11.0.0",
    "lodash-es": "^4.17.21",
    "pako": "^2.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@imminently/standard": "^0.2.5",
    "@swc/core": "^1.14.0",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.0",
    "@types/pako": "^2.0.4",
    "jest": "^29.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
