{
  "name": "@imminently/interview-ui",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/imminently/interview-sdk.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "bump:patch": "npm run build &&npm version patch --no-git-tag-version",
    "publish:patch": "npm run bump:patch && npm publish"
  },
  "dependencies": {
    "@imminently/interview-sdk": "workspace:^",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "motion": "^12.11.3",
    "react": "^19.0.0",
    "react-day-picker": "9.7.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.5",
    "vaul": "^1.1.2",
    "yup": "^1.6.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
