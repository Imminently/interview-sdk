{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "docs/.next/**"
      ]
    },
    "publish:patch": {
      "dependsOn": [],
      "outputs": []
    }
  }
}