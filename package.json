{
  "name": "interview-sdk",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/ui",
    "dev-app"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "install:all": "bun install && bun run build",
    "publish:patch": "turbo run publish:patch --concurrency=1"
  },
  "devDependencies": {
    "@imminently/interview-sdk": "workspace:*",
    "@imminently/interview-ui": "workspace:*",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^22.19.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "@tailwindcss/postcss": "^4.1.16",
    "tsup": "^8.5.0",
    "turbo": "^2.6.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "packageManager": "bun@1.2.17"
}
