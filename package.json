{
  "name": "interview-sdk",
  "private": true,
  "workspaces": [
    "packages/*",
    "dev-app"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "install:all": "npm install && npm run build",
    "publish:patch": "turbo run publish:patch --concurrency=1"
  },
  "devDependencies": {
    "@imminently/interview-sdk": "workspace:*",
    "@imminently/interview-ui": "workspace:*",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "@tailwindcss/postcss": "^4.1.11",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "packageManager": "bun@1.2.17"
}